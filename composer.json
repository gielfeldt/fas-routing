{
    "name": "fas/routing",
    "type": "library",
    "description": "Fast and simple router",
    "keywords": [
        "router",
        "psr",
        "middleware",
        "di",
        "container",
        "autowire"
    ],
    "homepage": "https://github.com/gielfeldt/fas-routing",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gielfeldt",
            "email": "thomas@gielfeldt.dk"
        }
    ],
    "provide": {
        "psr/http-server-handler-implementation": "^1.0",
        "psr/http-server-middleware-implementation": "^1.0"
    },
    "require": {
        "php": ">=7.4.0",
        "fas/di": "^0.1",
        "fas/exportable": "^0.1",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "nikic/fast-route": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Fas\\Routing\\": "src/Routing/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.6",
        "laminas/laminas-diactoros": "^2.6",
        "nyholm/psr7": "^1.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Fas\\Routing\\Tests\\": "tests/"
        }
    }
}
